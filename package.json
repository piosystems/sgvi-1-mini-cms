    "css-build": "node-sass --omit-source-map-url src/bulma/sass/**/ -o src/bulma/css/**/",
    "css-build-with-compress-option": "node-sass --omit-source-map-url src/bulma/sass/**/ -o src/bulma/css/**/ --output-style compressed",
    "minify": "gulp",
    "css": "npm run css-build && npm run minify",
    "copy-bulma-extension-js-to-public": "copyfiles -u 5 node_modules/**/bulma-accordion/dist/js/*.min.js node_modules/**/bulma-carousel/dist/js/*.min.js node_modules/**/bulma-calendar/dist/js/*.min.js public/js"
